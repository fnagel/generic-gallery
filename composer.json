{
  "name": "felixnagel/generic-gallery",
  "type": "typo3-cms-extension",
  "description": "TYPO3 CMS gallery extension.",
  "homepage": "https://github.com/fnagel/generic-gallery",
  "license": "GPL-2.0+",
  "support": {
    "issues": "https://github.com/fnagel/generic-gallery/issues"
  },
  "keywords": [
    "TYPO3 CMS",
    "gallery",
    "image",
    "slider",
    "lightbox",
    "extbase",
    "fluid"
  ],
  "require": {
    "php": ">=7.0,<8.0",
    "typo3/cms-core": ">=8.0.0,<=9.5.99"
  },
  "suggest": {
    "typo3/cms-filemetadata": "*",
    "fab/metadata": "^2.3",
    "causal/extractor": "^1.5"
  },
  "autoload": {
    "psr-4": {
      "FelixNagel\\GenericGallery\\": "Classes/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/TYPO3/TYPO3.CMS"
    }
  ]
}
